;; -*- mode: emacs-lisp; lexical-binding: t -*-

;; Prefer newer .el over stale .elc files
(setq load-prefer-newer t)

(eldev-use-package-archive 'melpa)

(setenv "PROJECTS_HOME" (expand-file-name "Developer/" (getenv "HOME")))

(eldev-use-local-dependency (expand-file-name "vulpea" (getenv "PROJECTS_HOME")))
(eldev-use-local-dependency (expand-file-name "vino" (getenv "PROJECTS_HOME")))
(eldev-use-local-dependency (expand-file-name "publicatorg" (getenv "PROJECTS_HOME")))
